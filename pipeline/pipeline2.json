{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "capa",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "SELECT *, GETUTCDATE() __dtts_extracao\nFROM bi.dbo.bi15a_movimento (nolock)\nWHERE bi15a_dt = '@{pipeline().parameters.process_date}'",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "gmcore_ds",
						"type": "DatasetReference",
						"parameters": {
							"database": "bi",
							"table": "bi15a_movimento"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "staging_ds",
						"type": "DatasetReference",
						"parameters": {
							"dirName": {
								"value": "bi15a_movimento_@{pipeline().parameters.process_date}",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "itens",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "SELECT b.*, GETUTCDATE() __dtts_extracao\nFROM bi.dbo.bi15a_movimento a (nolock)\n     JOIN bi.dbo.bi15b_movimento_itens b (nolock) \n           ON bi15a_id = bi15b_id_movimento\nWHERE bi15a_dt = '@{pipeline().parameters.process_date}'",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "gmcore_ds",
						"type": "DatasetReference",
						"parameters": {
							"database": "bi",
							"table": "bi15b_movimento_itens"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "staging_ds",
						"type": "DatasetReference",
						"parameters": {
							"dirName": {
								"value": "bi15b_movimento_itens_@{pipeline().parameters.process_date}",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"process_date": {
				"type": "string"
			}
		},
		"annotations": []
	}
}